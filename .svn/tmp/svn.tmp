					ng-model="paciente.telefono" ng-change="validartlf(paciente.telefono)" required>
			</div>
			<div class="col-sm-2">
			<label class="control-label">Telefono Celular:</label>
				<input type="text" class="form-control" maxlength="9"
					placeholder="Ingrese celular" 
					ng-model="paciente.celular" ng-change="validarcel(paciente.celular)">
			</div>
			</div>
			<div class="form-group">
			<div class="col-sm-2"></div>
			<div class="col-sm-2">
			<label class="control-label">Estado civil:</label>
				<select class="form-control" ng-model="paciente.estadocivil"
					ng-options="value.id as value.value  for value in estadoscivil">
					<option>--</option>
				</select>
			</div>
			<div class="col-sm-2">
			<label class="control-label">Religion:</label>
				<select class="form-control" ng-model=""
					ng-options="">
					<option>--</option>
				</select>
			</div>
			</div>
			<div class="form-group">
			<div class="col-sm-2"></div>
			<div class="col-md-2">
			<label class="control-label"> Grado instruccion: </label>
				<select class="form-control" ng-model="paciente.idgradoinstitucion"
					ng-options="value.idgradoinstitucion as value.descripcion	  for value in gradoinstitucions">
					<option>--</option>
				</select> <span
					ng-show="myForm.idgradoinstitucion.$dirty && myForm.idgradoinstitucion.$invalid"
					class="help-inline">idgradoinstitucion requerido!</span>
			</div>
			<div class="col-md-2">		
			<label class="control-label"> OcupaciÃ³n:
			</label>
				<select class="form-control" ng-model="paciente.idocupacion"
					ng-options="value.idocupacion as value.descripcion  for value in ocupacions">
					<option>--</option>
				</select> <span
					ng-show="myForm.idocupacion.$dirty && myForm.idocupacion.$invalid"
					class="help-inline">idocupacion requerido!</span>
			</div>
			</div>
			</fieldset>
			<fieldset>
			<legend>Referencias</legend>
			<div class="form-group">
			<div class="col-sm-2"></div>
			<div class="col-sm-8">
			<label class="control-label">DirecciÃ³n:</label>
				<input type="text" class="form-control"
					placeholder="Ingrese direccion" ng-model="paciente.direccion">
			</div>
			</div>
			<div class="form-group">
			<div class="col-sm-2"></div>
			<div class="col-sm-8">
			<label class="control-label">Referencia</label>
				<input type="text" class="form-control"
					placeholder="Ingrese referencia" ng-model="paciente.referencia">
			</div>
			</div>
			<div class="form-group">
			<div class="col-sm-2"></div>
			<div class="col-md-2">
			<label class="control-label">Empresa:</label>
				<select class="form-control" ng-model=""
					ng-options="">
					<option>--</option>
				</select> <span
					ng-show="myForm.idocupacion.$dirty && myForm.idocupacion.$invalid"
					class="help-inline"> requerido!</span>
			</div>
			</div>
			<div class="form-group">
			<div class="col-sm-2"></div>
			<div class="col-md-4">
			<label class="control-label">En caso sea necesario llamar a:</label>
			</div>
			</div>
			<div class="form-group">
			<div class="col-sm-2"></div>
			<div class="col-sm-2">
			<label class="control-label">Nombre:</label>
				<input type="text" class="form-control"
					placeholder="Ingrese nombre" ng-model="">
			</div>
			<div class="col-sm-2">
			<label class="control-label">Telefono:</label>
				<input type="text" class="form-control"
					placeholder="Ingrese telefono" ng-model="">
			</div>
			</div>
			<div class="form-group">
			<div class="col-sm-2"></div>
			<div class="col-md-2">
			<label class="control-label"> Parentezco: </label>
				<select class="form-control" ng-model="paciente.idgradoinstitucion"
					ng-options="value.idgradoinstitucion as value.descripcion	  for value in lsParentezco">
					<option>--</option>
				</select> <span
					ng-show="myForm.idgradoinstitucion.$dirty && myForm.idgradoinstitucion.$invalid"
					class="help-inline"> requerido!</span>
			</div>
			<div class="col-sm-2">
			<label class="control-label">Ubicacion de:</label>
				<input type="text" class="form-control"
					placeholder="Ingrese telefono" ng-model="">
