	<form name="formValidateAma" class="form-horizontal form-validation">
	
<fieldset>
	<legend>Enfermedad Actual
		<label class="cruds" ng-click="crudAnamnesis(1, 2)">
			<span style="color:blue; font-size: 10pt">{{anamnesis.idanamnesis==0?'Insertar Enfermedad':'Actualizar Enfermedad'}}</span>
		</label>
		<button style="float:right ;margin-bottom:5px" type="submit" ng-disabled="formValidateAma.$invalid"
				ng-click="crudtotal()"
				class="btn btn-sm btn-success btn-addon" tooltip-placement="right"
				tooltip="{{titulopaciente}} antecedentes.">
				<i class="glyphicon glyphicon-refresh"></i>{{anamnesis.idnamnesis==0?'Inserta':'Actualización General'}}
		</button>
	</legend>
		<div class="form-group">
		<div class="col-sm-1"></div>
			<div class="col-sm-6">
				<label class=" control-label">Motivo de Consulta:</label>
				<input class="form-control" placeholder="Ingrese motivo de consulta" ng-model="anamnesis.motv_consulta"/>
			</div>
		</div>
		<div class="form-group">
		<div class="col-sm-1"></div>
			<div class="col-sm-6">
				<label class=" control-label">Signos y sintomas principales:</label>
				<input class="form-control" placeholder="Ingrese signos y sintomas principales" ng-model="anamnesis.sintomas_principales"/>
			</div>
		</div>
</fieldset>
<fieldset>
	<legend>Antecedentes
